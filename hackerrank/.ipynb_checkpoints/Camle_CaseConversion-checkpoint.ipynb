   "source": [
    "n = input()\n",
    "n = n.split(\" \")\n",
    "l1 = []\n",
    "v = ''\n",
    "for i in n:\n",
    "    if(type(i) == str and len(i) != 0):\n",
    "        v = i[0].upper()+i[1:]\n",
    "        l1.append(v)\n",
    "    else:\n",
    "        l1.append(i)\n",
    "\n",
    "n = (\" \".join(l1))\n",
    "n"
   ]
